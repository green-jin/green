    <typegroup name="Component Items">
	  <itemtype code="FooterNavigationComponent" autocreate="false" generate="false" >
	  <description>CMS component for footer navigation in the accelerator.</description>
		  <attributes>
			<attribute qualifier="serviceStatement" type="localized:java.lang.String">
				<description>Intended to store a copyright notice or other text to be displayed in the footer</description>
				<persistence type="property" />
				<modifiers read="true" write="true" optional="true" />
			</attribute>
			<attribute qualifier="serviceStatementUrlLike" type="java.lang.String">
				<description>Intended to store a copyright notice or other text to be displayed in the footer</description>
				<persistence type="property" />
				<modifiers read="true" write="true" optional="true" />
			</attribute> 
		  </attributes>
	  </itemtype> 
      <itemtype code="HomeSimpleBannerComponent" generate="true"
        jaloclass="com.ncip.core.jalo.components.HomeSimpleBannerComponent"
        extends="SimpleResponsiveBannerComponent" autocreate="true">
        <description>Concrete implementation of abstract responsive banner component (經銷商專區 ,我們的保證
          Component)
        </description>
        <attributes>
          <attribute qualifier="subTitleTextHidden" type="java.lang.Boolean">
            <defaultvalue>Boolean.FALSE</defaultvalue>
            <persistence type="property"/>
            <modifiers optional="false"/>
          </attribute>
          <attribute qualifier="subTitleText" type="localized:java.lang.String">
            <persistence type="property"/>
            <modifiers read="true" write="true" optional="true"/>
            <description>subTitleText.</description>
          </attribute>
        </attributes>
      </itemtype>
      <!-- 經銷商專區  Container-->
      <itemtype code="BrandContainerComponent" generate="true"
        jaloclass="com.ncip.core.jalo.components.BrandContainerComponent"
        extends="SimpleCMSComponent" autocreate="true">
        <description>It represents Selected Brand Container component. (經銷商專區 ,我們的保證 Container)
        </description>
        <attributes>
          <attribute qualifier="mainTitleTextHidden" type="java.lang.Boolean">
            <defaultvalue>Boolean.FALSE</defaultvalue>
            <persistence type="property"/>
            <modifiers optional="false"/>
          </attribute>
          <attribute qualifier="maintTitleText" type="localized:java.lang.String">
            <persistence type="property"/>
            <modifiers read="true" write="true" optional="true"/>
            <description>maintTitleText.</description>
          </attribute>
          <attribute qualifier="simpleBannerComponent" type="HomeSimpleBannerComponentList">
            <persistence type="property"/>
            <modifiers read="true" write="true" optional="true"/>
            <description>It represents BrandCarousel collection component.</description>
          </attribute>
          <attribute qualifier="height" type="java.lang.String">
            <persistence type="property"/>
            <modifiers read="true" write="true" optional="true"/>
            <description>It represents BrandCarousel collection component.</description>
          </attribute>
          <attribute qualifier="width" type="java.lang.String">
            <persistence type="property"/>
            <modifiers read="true" write="true" optional="true"/>
            <description>It represents BrandCarousel collection component.</description>
          </attribute>
          <attribute qualifier="size" type="java.lang.Integer">
            <persistence type="property"/>
            <modifiers read="true" write="true" optional="false"/>
            <description>Have size component Each Row.</description>
            <defaultvalue>4</defaultvalue>
          </attribute>
        </attributes>
      </itemtype>
